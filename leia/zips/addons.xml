<addon id="plugin.video.itv" name="ITV" version="3.0.2" provider-name="LittleWiz, Cowebtech">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.httplib2"/>
    <import addon="script.module.beautifulsoup4" />
    <import addon="script.module.six" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
  </extension>
  <extension library="service.py" point="xbmc.service"  start="startup"/>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <summary>TV from ITV Hub</summary>
    <description>This add-on enables the playing of Catchup TV from ITV Hub (UK only)</description>
    <disclaimer lang="en">This add-on is a front end to the ITV Hub website. It is not an official ITV product. A UK TV Licence is required to watch the live, catch-up, or on-demand TV content.</disclaimer>
    <assets>
      <icon>icon.png</icon>
  </assets>
  </extension>
</addon>

